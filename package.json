{
  "name": "vitepress-demo-editor",
  "version": "3.0.2",
  "description": "vitepress 插件,支持在线编辑 Demo 演示源代码,支持 SFC, JSX, TSX",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./markdownPlugin": {
      "require": "./markdownPlugin/index.js",
      "import": "./markdownPlugin/index.js",
      "type": "./markdownPlugin.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "vitepress",
    "plugin",
    "vitepress-plugin",
    "demo",
    "editor",
    "playground"
  ],
  "files": [
    "dist",
    "markdownPlugin"
  ],
  "homepage": "https://github.com/liyao1520/vitepress-demo-editor",
  "repository": {
    "type": "git",
    "url": "git@github.com:liyao1520/vitepress-demo-editor.git"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "build": "vite build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/babel__standalone": "^7.1.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/markdown-it-container": "^2.0.5",
    "@types/node": "^18.0.5",
    "@vitejs/plugin-vue": "^3.0.1",
    "vite": "^3.0.2",
    "vite-plugin-dts": "^1.3.0",
    "vitepress": "^1.0.0-alpha.4"
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-typescript": "^7.18.8",
    "@babel/standalone": "^7.18.8",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/runtime-core": "^3.2.40",
    "@vue/runtime-dom": "^3.2.40",
    "browser-assert": "^1.2.1",
    "markdown-it-container": "^3.0.0",
    "monaco-editor-ex": "^0.33.8",
    "path-browserify": "^1.0.1",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vitepress": ">=0.1.0",
    "vue": ">=3.0.0"
  }
}
